{
  "title": "设计模式",
  "description": "设计模式学习中的总结,主要语言是Java",
  "author": "Evan",
  "language": "zh-hans",
  "gitbook": "3.2.3",
  "links": {
    "sidebar": {
      "关于我": "https://jiapeng.ink"
    }
  },
  "plugins": [
    "theme-comscore",
    "-lunr",
    "-search",
    "search-plus",
    "disqus",
    "-highlight",
    "prism",
    "prism-themes",
    "github-buttons",
    "splitter",
    "-sharing",
    "sharing-plus",
    "tbfed-pagefooter",
    "expandable-chapters-small",
    "donate",
    "sitemap-general",
    "copy-code-button",
    "alerts"
  ],
  "pluginsConfig": {
    "theme-default": {
        "showLevel": true
    },
    "disqus": {
      "shortName": "evanljp"
    },
    "prism": {
      "css": [
        "prism-themes/themes/prism-atom-dark.css"
      ]
    },
    "github-buttons": {
      "buttons": [
        {
          "user": "evanljp",
          "repo": "design-pattern",
          "type": "star",
          "count": true,
          "size": "small"
        }, {
          "user": "evanljp",
          "type": "follow",
          "width": "120",
          "count": false,
          "size": "small"
        }
      ]
    },
    "sharing": {
      "facebook": true,
      "weibo": true,
      "all": [
        "douban",
        "google",
        "linkedin",
        "qq",
        "twitter",
        "weibo"
      ]
    },
    "tbfed-pagefooter": {
      "copyright": "<a href='https://github.com/evanljp'>Evan</a>",
      "modify_label": "该文件修订时间：",
      "modify_format": "YYYY-MM-DD HH:mm:ss"
    },
    "donate": {
      "wechat": "https://raw.githubusercontent.com/EvanLjp/evanljp.github.io/master/img/wechat.jpeg",
      "alipay": "https://raw.githubusercontent.com/EvanLjp/evanljp.github.io/master/img/alipay.jpeg",
      "title": "",
      "button": "打赏",
      "alipayText": "支付宝打赏给evan",
      "wechatText": "微信打赏给evan"
    },
    "sitemap-general": {
      "prefix": "http://www.evanljp.ink/design-pattern"
    }
  }
}
